$(document).on("click", ".vid-followup .button.yes", function() {
  $(".vid-followup .prompt").text("Hooray! :)");
  $(".vid-followup .button").remove();
  $(".vid-followup").append("<textarea placeholder='Optional: tell us what you liked about it here...'></textarea><button vote='yes' class='button submit'>Send feedback</button>");
  WH.maEvent("summary_video_events", {"event":"help_yes", "article":window.location.pathname, "version":machinifyExpVersion}, true);
});

$(document).on("click", ".vid-followup .button.no", function() {
  $(".vid-followup .prompt").text("Uh oh :( How could it be better?");
  $(".vid-followup .button").remove();
  $(".vid-followup").append("<textarea placeholder='Your comments here...'></textarea><button vote='no' class='button submit'>Send feedback</button>");
  WH.maEvent("summary_video_events", {"event":"help_no", "article":window.location.pathname, "version":machinifyExpVersion}, true);
});

$(document).on("click", ".vid-followup .button.submit", function() {
  $(".vid-followup .prompt").html("Thanks for your feedback.<br><br>Keep reading below for more tips.");
  //WH.maEvent("summary_video_feedback", {"vote":$(this).attr("vote"), "feedback":$(".vid-followup textarea").val(), "article":window.location.pathname}, true);  
  WH.maEvent("summary_video_events", {"event":"feedback_" + $(this).attr("vote"), "feedback":$(".vid-followup textarea").val(), "article":window.location.pathname, "version":machinifyExpVersion}, true);
  $(".vid-followup .button").remove(); 
  $(".vid-followup textarea").remove();
  $(".vid-followup .read-more").remove();
});

