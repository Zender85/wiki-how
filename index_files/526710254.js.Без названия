$(document).on("click", ".signup-cta .feature-idea:not(.selected)", function() {
  WH.maEvent("test_268_1_progress_next_steps", {"button":whichButton, "feature-idea":$(this).text(), "article":window.location.pathname, "variation":"3 progress states - all steps"}, true);
  $(this).addClass("selected");
  if ($(this).text() == "Read success stories from other people about this article") {
   window.location.hash = '#sp_star_box';
  }
  if ($(this).text() == "Share your success story with other people about this article") {
	 mw.loader.using('ext.wikihow.UserReviewForm', function () {
   	window.WH.UserReviewForm.prototype.loadUserReviewForm();
	 });
  }
  if ($(this).text() == "Upload a photo of your progress with this article") {
   window.location.hash = '#uci_images_section';
  }
  if ($(this).text() == "Read answered questions from other readers about this article") {
   window.location.hash = '#Questions_and_Answers_sub';
  }
  if ($(this).text() == "Ask other readers a question about this article") {
   window.location.hash = '#qa_ask_heading';
  }
  if ($(this).text() == "Answer another reader’s question about this article") {
   window.location.hash = '#qa_submitted_questions_container';
  }
  if ($(this).text() == "Test your knowledge about this article") {
   window.location.hash = '#qz_anchor_1';
  }
  if ($(this).text() == "Read other articles related to this article") {
   window.location.hash = '#relatedwikihows';
  }
  if ($(this).text() == "Tell us if this article was helpful or not") {
   window.location.hash = '#rating_container';
  }
});

