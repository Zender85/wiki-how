$(window).bind('scroll', function() {
  if(isScrolledIntoView($('.summarysection video')) && NotinVid && isPlaying) {
    //console.log("vid playing in view");
    window['optimizely'] = window['optimizely'] || [];
    window.optimizely.push({
      type: "event",
      eventName: "summary_vid_view_start"
    });
    NotinVid = false;
   
  } else if ((isScrolledIntoView($('.summarysection video')) === false) && (NotinVid === false) && isPlaying) {
    //console.log("scrolled off vid while playing");
    window.optimizely.push({
      type: "event",
      eventName: "summary_vid_view_scroll_away_while_playing"
    });
    WH.maEvent("summary_video_events", {"event":"scroll_away_while_playing", "article":window.location.pathname, "version":machinifyExpVersion}, true);
    NotinVid = true;
  }
});

var isPlaying = false;
var playTimer = 0;
var playCounterInterval;

$(".summarysection video")[0].addEventListener("playing", function() {
  console.log("playing");
  isPlaying = true;
  NotinVid = false;   //NEW
  $(".vid-followup").remove();
  playCounterInterval = setInterval(function() {
		playTimer++;
    //console.log(playTimer);
}, 1000);
  WH.maEvent("summary_video_events", {"event":"play", "article":window.location.pathname, "version":machinifyExpVersion}, true);
  window['optimizely'] = window['optimizely'] || [];
  window['optimizely'].push({
    type: "event",
    eventName: "summary_video_play"
  });
}, true);

$(".summarysection video")[0].addEventListener("pause", function() { // user paused or video ended
  //console.log("paused");
  var seconds = playTimer;
  isPlaying = false;
  WH.maEvent("summary_video_events", {"seconds":seconds, "event":"pause", "article":window.location.pathname, "version":machinifyExpVersion}, true);
  playTimer = 0;
  clearInterval(playCounterInterval);
  window['optimizely'] = window['optimizely'] || [];
  window['optimizely'].push({
    type: "event",
    eventName: "summary_video_pause_or_end"
  });
}, true);

$(".summarysection video")[0].addEventListener("ended", function() {  // pause event apparently gets sent at same time
  //console.log("played to completion");
  isPlaying = false;
  if ($(".vid-followup").length == 0) {
  	$("#inahurry .summarysection.mwimg").append("<div class='vid-followup'><div class='prompt'>Did this help you?</div><button class='button yes'>Yes</button><button class='button no'>No</button><div class='read-more'>Keep reading below to learn more...</div></div>");  
  }
  WH.maEvent("summary_video_events", {"event":"ended", "article":window.location.pathname, "version":machinifyExpVersion}, true);
  window['optimizely'] = window['optimizely'] || [];
  window['optimizely'].push({
    type: "event",
    eventName: "summary_video_play_to_end"
  });
  if (!NotinVid) {
    window['optimizely'].push({
      type: "event",
      eventName: "summary_video_play_to_end_in_view"
    });
    WH.maEvent("summary_video_events", {"event":"ended_in_view", "article":window.location.pathname, "version":machinifyExpVersion}, true);
    console.log("ended in view");
  }
}, true);

/*
